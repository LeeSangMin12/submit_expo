{
  "expo": {
    "name": "서브밋",
    "slug": "submit_expo",
    "scheme": "com.submitexpo.rn",
    "version": "1.0.17",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "icon": "./src/assets/notification_icon.png",
      "color": "#59ff59"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "buildNumber": "1.0.17",
      "supportsTablet": true,
      "bundleIdentifier": "com.submitexpo.rn",
      "entitlements": {
        "com.apple.developer.applesignin": ["Default"]
      },
      "infoPlist": {
        "CFBundleDevelopmentRegion": true
      }
    },
    "android": {
      "versionCode": 17,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.submitexpo.rn"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "locales": {
      "ko": "./locales/ko.json"
    },
    "extra": {
      "eas": {
        "projectId": "db17479e-bf3f-42f9-a0aa-82c416e12768"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 33,
            "targetSdkVersion": 33,
            "buildToolsVersion": "33.0.0"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "프로필 사진 설정 및 변경을 위해 카메라 권한을 허락해 주세요."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./src/assets/notification_icon.png"
        }
      ]
    ]
  }
}
